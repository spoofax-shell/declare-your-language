module trans/semantics/semantics

imports trans/semantics/interpret


// Entry point

signature
  arrows
    Program -init-> V
    Expr -init-> V

  native operators
    mkNullThis: This

rules
  
  p : Program -init-> v
  where
    C {}, Env {}, This mkNullThis() |- p :: Store {} --> v :: Store _.    

  e : Expr -init-> v
  where
    C {}, Env {}, This mkNullThis() |- e :: Store {} --> v :: Store _.   
